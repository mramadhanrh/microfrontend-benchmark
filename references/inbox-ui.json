          },
          {
            "title": "ADD-ONS",
            "items": [
              {
                "icon": "users",
                "label": "Web Visitors"
              }
            ]
          },
          {
            "title": "INTEGRATIONS",
            "items": [
              {
                "icon": "message",
                "label": "Messaging",
                "badge": "New",
                "badgeColor": "orange"
              }
            ]
          }
        ]
      },
      {
        "type": "header",
        "height": "60px",
        "backgroundColor": "#FFFFFF",
        "elements": [
          {
            "type": "breadcrumb",
            "items": ["Leads", "Leads Inbox"]
          },
          {
            "type": "search",
            "placeholder": "Search here...",
            "position": "center"
          },
          {
            "type": "actions",
            "position": "right",
            "items": [
              {
                "type": "icon-button",
                "icon": "sun"
              },
              {
                "type": "icon-button",
                "icon": "moon"
              },
              {
                "type": "user-profile",
                "name": "Jawad Rahman",
                "subtitle": "Filllo Agency",
                "avatar": "url"
              }
            ]
          }
        ]
      },
      {
        "type": "main-content",
        "backgroundColor": "#F5F5F7",
        "padding": "24px",
        "sections": [
          {
            "type": "page-header",
            "title": "Leads Inbox",
            "actions": [
              {
                "type": "tab-group",
                "tabs": [
                  {
                    "label": "Inbox",
                    "badge": "8",
                    "active": true
                  },
                  {
                    "label": "Archive",
                    "icon": "archive"
                  }
                ]
              },
              {
                "type": "dropdown",
                "icon": "label",
                "label": "All labels"
              },
              {
                "type": "dropdown",
                "label": "Everyone"
              },
              {
                "type": "button",
                "variant": "primary",
                "label": "Add Lead",
                "icon": "plus",
                "backgroundColor": "#2D1B69"
              },
              {
                "type": "icon-button",
                "icon": "more"
              }
            ]
          },
          {
            "type": "data-table",
            "backgroundColor": "#FFFFFF",
            "borderRadius": "8px",
            "columns": [
              {
                "key": "checkbox",
                "type": "checkbox",
                "width": "40px"
              },
              {
                "key": "title",
                "label": "Title",
                "sortable": false,
                "width": "150px"
              },
              {
                "key": "owner",
                "label": "Owner",
                "sortable": true,
                "width": "180px",
                "renderer": "user-avatar"
              },
              {
                "key": "labels",
                "label": "Labels",
                "width": "120px",
                "renderer": "badge"
              },
              {
                "key": "source",
                "label": "Source",
                "width": "120px",
                "renderer": "icon-text"
              },
              {
                "key": "leadCreated",
                "label": "Lead created",
                "width": "180px",
                "renderer": "date-time"
              },
              {
                "key": "nextActivity",
                "label": "Next activity",
                "width": "120px",
                "renderer": "status-text"
              },
              {
                "key": "actions",
                "type": "menu",
                "width": "40px"
              }
            ],
            "rows": [
              {
                "title": "olux Lead",
                "owner": {
                  "name": "Kristin Watson",
                  "avatar": "url"
                },
                "labels": {
                  "text": "Warm",
                  "color": "yellow"
                },
                "source": {
                  "icon": "target",
