            "rowHeight": "56px",
            "hoverEffect": true
          }
        ]
      }
    ],
    "colorScheme": {
      "primary": "#2D1B69",
      "background": "#F5F5F7",
      "surface": "#FFFFFF",
      "text": {
        "primary": "#000000",
        "secondary": "#666666"
      },
      "status": {
        "warm": "#FB8C00",
        "cold": "#1E88E5",
        "active": "#00897B",
        "noActivity": "#E8232C",
        "push": "#FB8C00"
      }
    }
  }
}
