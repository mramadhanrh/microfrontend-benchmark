{
  "name": "home_remote",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/microfrontend/remotes/home_remote/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/microfrontend/remotes/home_remote",
        "index": "apps/microfrontend/remotes/home_remote/src/index.html",
        "baseHref": "/",
        "main": "apps/microfrontend/remotes/home_remote/src/main.ts",
        "tsConfig": "apps/microfrontend/remotes/home_remote/tsconfig.app.json",
        "assets": [
          "apps/microfrontend/remotes/home_remote/src/favicon.ico",
          "apps/microfrontend/remotes/home_remote/src/assets"
        ],
        "styles": ["apps/microfrontend/remotes/home_remote/src/styles.css"],
        "scripts": [],
        "webpackConfig": "apps/microfrontend/remotes/home_remote/webpack.config.ts",
        "postcssConfig": "apps/microfrontend/remotes/home_remote/postcss.config.js"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/microfrontend/remotes/home_remote/src/environments/environment.ts",
              "with": "apps/microfrontend/remotes/home_remote/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "webpackConfig": "apps/microfrontend/remotes/home_remote/webpack.config.prod.ts"
        }
      }
    },
    "serve": {
      "executor": "@nx/react:module-federation-dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "home_remote:build",
        "hmr": true,
        "port": 4300
      },
      "configurations": {
        "development": {
          "buildTarget": "home_remote:build:development"
        },
        "production": {
          "buildTarget": "home_remote:build:production",
          "hmr": false
        }
      }
    },
    "serve-static": {
      "executor": "@nx/react:module-federation-static-server",
      "defaultConfiguration": "production",
      "options": {
        "serveTarget": "home_remote:serve"
      },
      "configurations": {
        "development": {
          "serveTarget": "home_remote:serve:development"
        },
        "production": {
          "serveTarget": "home_remote:serve:production"
        }
      }
    }
  }
}
